                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT TPT_JOB_MAIN_Tbl.JOB_NUMBER, TPT_JOB_MAIN_Tbl.BOOKING_DATE, TPT_JOB_MAIN_Tbl.CUST_CODE, TPT_JOB_MAIN_Tbl.TPT_DEPT_CODE, 
TPT_JOB_MAIN_Tbl.JOB_TYPE, sum(TPT_JOB_DETAIL_CARGO_Tbl.GROSS_CBM) as grosscbm, sum(TPT_JOB_DETAIL_CARGO_Tbl.GROSS_WEIGHT) as grossweight, 
ACT_CustVend_Master_Tbl.CustVend_Name
FROM TPT_JOB_DETAIL_CARGO_Tbl INNER JOIN
TPT_JOB_MAIN_Tbl ON TPT_JOB_DETAIL_CARGO_Tbl.JOB_ID = TPT_JOB_MAIN_Tbl.JOB_ID INNER JOIN
ACT_CustVend_Master_Tbl ON TPT_JOB_MAIN_Tbl.CUST_CODE = ACT_CustVend_Master_Tbl.CustVend_Code

WHERE 
(TPT_JOB_MAIN_Tbl.JOB_NUMBER not in
	(select inv.Job_Number from ACT_IV_Transport_Invoice_Master_Tbl 
	as inv where inv.Job_Type_Code = 'HL')) and 
(TPT_JOB_MAIN_Tbl.BOOKING_DATE &gt;= @FROM_DATE) AND
(TPT_JOB_MAIN_Tbl.BOOKING_DATE &lt;= @TO_DATE) 

group by TPT_JOB_MAIN_Tbl.JOB_NUMBER, TPT_JOB_MAIN_Tbl.BOOKING_DATE, TPT_JOB_MAIN_Tbl.CUST_CODE, TPT_JOB_MAIN_Tbl.TPT_DEPT_CODE, 
TPT_JOB_MAIN_Tbl.JOB_TYPE,ACT_CustVend_Master_Tbl.CustVend_Name

order by TPT_JOB_MAIN_Tbl.BOOKING_DATE,TPT_JOB_MAIN_Tbl.JOB_NUMBER</CommandText>
