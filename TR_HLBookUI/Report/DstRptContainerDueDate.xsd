                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select TO_DATE,TO_TIME,CONTAINER_NUMBER,TO_STOP as CY,'' as Port,
b.CustVend_Name,b.Address1,JOB_NUMBER
from TPT_JOB_DETAIL_CARGO_Tbl
inner join TPT_JOB_MAIN_Tbl
on TPT_JOB_DETAIL_CARGO_Tbl.job_id = TPT_JOB_MAIN_Tbl.job_id
inner join CRT_Operation_Database_Tbl a
on TPT_JOB_DETAIL_CARGO_Tbl.TO_STOP = a.Operation_Code
inner join ACT_CustVend_Master_Tbl b
on TPT_JOB_MAIN_Tbl.CUST_CODE = b.CustVend_Code
where a.Operation_Type_Code='CY'  AND 
(TPT_JOB_DETAIL_CARGO_Tbl.TO_DATE &gt;= @FROM_DATE) AND
(TPT_JOB_DETAIL_CARGO_Tbl.TO_DATE &lt;= @TO_DATE)
AND TPT_JOB_DETAIL_CARGO_Tbl.STATUS&lt;&gt;4
AND TPT_JOB_DETAIL_CARGO_Tbl.CONTAINER_NUMBER&lt;&gt;''
union
select TO_DATE,TO_TIME,CONTAINER_NUMBER,'' as CY,TO_STOP as Port,
b.CustVend_Name,b.Address1,JOB_NUMBER
from TPT_JOB_DETAIL_CARGO_Tbl
inner join TPT_JOB_MAIN_Tbl
on TPT_JOB_DETAIL_CARGO_Tbl.job_id = TPT_JOB_MAIN_Tbl.job_id
inner join CRT_Operation_Database_Tbl a
on TPT_JOB_DETAIL_CARGO_Tbl.TO_STOP = a.Operation_Code
inner join ACT_CustVend_Master_Tbl b
on TPT_JOB_MAIN_Tbl.CUST_CODE = b.CustVend_Code
where a.Operation_Type_Code='PO'   AND
(TPT_JOB_DETAIL_CARGO_Tbl.TO_DATE &gt;= @FROM_DATE) AND
(TPT_JOB_DETAIL_CARGO_Tbl.TO_DATE &lt;= @TO_DATE)
AND TPT_JOB_DETAIL_CARGO_Tbl.STATUS&lt;&gt;4
AND TPT_JOB_DETAIL_CARGO_Tbl.CONTAINER_NUMBER&lt;&gt;''</CommandText>
