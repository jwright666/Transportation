              <DbSource ConnectionRef="LOCAL_PC (Settings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT 
CUST_CODE,CustVend_Name,Booked_DateTime as BOOKING_DATE ,JOB_NUMBER,TRIP_TYPE as JOB_TYPE,TPT_DEPT_CODE,
JOB_URGENCY_TYPE,Status,sum(BOOK_VOLUME) as VOLUME,
sum(BOOK_WEIGHT) as WEIGHT,
BILLING_UOM
FROM TRK_JOB_MAIN_Tbl INNER JOIN
TRK_JOB_DETAIL_TRIP_Tbl ON TRK_JOB_DETAIL_TRIP_Tbl.JOB_ID = TRK_JOB_MAIN_Tbl.JOB_ID
INNER JOIN ACT_CustVend_Master_Tbl 
ON TRK_JOB_MAIN_Tbl.CUST_CODE = ACT_CustVend_Master_Tbl.CustVend_Code 


WHERE 
((TRK_JOB_MAIN_Tbl.Booked_DateTime &gt;= @FROM_DATE) AND
(TRK_JOB_MAIN_Tbl.Booked_DateTime &lt;= @TO_DATE) AND
(TRK_JOB_MAIN_Tbl.CUST_CODE&gt;=@FROM_SUBCON) AND
(TRK_JOB_MAIN_Tbl.CUST_CODE&lt;=@TO_SUBCON))

GROUP BY CUST_CODE,CustVend_Name,Booked_DateTime,JOB_NUMBER, TRIP_TYPE,TPT_DEPT_CODE,
JOB_URGENCY_TYPE,BILLING_UOM,Status

ORDER BY CUST_CODE,Booked_DateTime



</CommandText>
