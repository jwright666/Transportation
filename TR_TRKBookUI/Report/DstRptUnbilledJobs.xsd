                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT TRK_JOB_MAIN_Tbl.JOB_NUMBER, TRK_JOB_MAIN_Tbl.BOOKED_DATETIME as BOOKING_DATE, TRK_JOB_MAIN_Tbl.CUST_CODE, TRK_JOB_MAIN_Tbl.TPT_DEPT_CODE, 
TRK_JOB_MAIN_Tbl.TRIP_TYPE as JOB_TYPE, sum(TRK_JOB_DETAIL_TRIP_Tbl.BOOK_VOLUME) as booked_volume,
sum(BOOK_WEIGHT) as booked_weight, 
ACT_CustVend_Master_Tbl.CustVend_Name,
TRK_JOB_DETAIL_TRIP_Tbl.billing_uom

FROM TRK_JOB_DETAIL_TRIP_Tbl INNER JOIN
TRK_JOB_MAIN_Tbl ON TRK_JOB_DETAIL_TRIP_Tbl.JOB_ID = TRK_JOB_MAIN_Tbl.JOB_ID INNER JOIN
ACT_CustVend_Master_Tbl ON TRK_JOB_MAIN_Tbl.CUST_CODE = ACT_CustVend_Master_Tbl.CustVend_Code

WHERE 
(TRK_JOB_MAIN_Tbl.JOB_NUMBER not in(select inv.Job_Number from ACT_IV_Transport_Invoice_Master_Tbl as inv where inv.Job_Type_Code = 'TR'))
and
(TRK_JOB_MAIN_Tbl.BOOKED_DATETIME &gt;= @FROM_DATE) AND
(TRK_JOB_MAIN_Tbl.BOOKED_DATETIME &lt;= @TO_DATE) 

group by TRK_JOB_MAIN_Tbl.JOB_NUMBER, TRK_JOB_MAIN_Tbl.BOOKED_DATETIME, TRK_JOB_MAIN_Tbl.CUST_CODE, TRK_JOB_MAIN_Tbl.TPT_DEPT_CODE, 
TRK_JOB_MAIN_Tbl.TRIP_TYPE,ACT_CustVend_Master_Tbl.CustVend_Name,
TRK_JOB_DETAIL_TRIP_Tbl.billing_uom

order by TRK_JOB_MAIN_Tbl.BOOKED_DATETIME,TRK_JOB_MAIN_Tbl.JOB_NUMBER</CommandText>
